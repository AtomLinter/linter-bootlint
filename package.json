{
  "name": "linter-bootlint",
  "main": "./lib/init.js",
  "version": "1.0.2",
  "keywords": [
    "html",
    "lint",
    "linter",
    "bootlint"
  ],
  "description": "Lint Bootstrap HTML on the fly, using bootlint.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomLinter/linter-bootlint.git"
  },
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-bootlint/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-bootlint#readme",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "apm test"
  },
  "dependencies": {
    "bootlint": "~0.14.2",
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.14.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-console": "off",
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ],
      "import/no-extraneous-dependencies": "off"
    },
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
